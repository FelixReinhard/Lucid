struct List{ls}

fn List::add(self, elem) => push(self.ls, elem);
fn List::get(self) => self.ls;
fn List::rev(self) {
  let new_ls = [];
  for i in self.ls {
    push(new_ls, i);
  }
  return new List(new_ls);
}

// create methods always at the very end.
fn List::create() => new List([]);


let x = List.create();
for i in range(10) {
  x.add(i);
}
print(x.rev());
